/* LABELS.MSS CONTENTS:
 * - place names
 */

/* Font sets are defined in palette.mss */



#building-text {
  [zoom >= 12] {
    text-name: "[name]";
    text-size: 14;
    text-fill: #FFFFFF;
    text-face-name: @sans;
    text-halo-radius: 1.5;
    text-wrap-width: 60;
    text-halo-fill: #444;
    text-placement: interior;
  }
}

